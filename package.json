    "@types/node": "^13.7.7",
    "@types/omit-empty": "^1.0.0",
    "ava": "^3.5.0",
    "eslint-config-richienb": "^0.3.0",
    "ts-node": "^8.6.2",
    "typedoc": "^0.16.11",
    "typescript": "^3.8.3",
    "xo": "^0.26.1"
  },
  "resolutions": {
    "eslint": "^6.8.0"
  },
  "xo": {
    "extends": "richienb/ts"
